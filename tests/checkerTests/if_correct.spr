b0:boolean := 2^10 = 1024; // true
b1:boolean := 2^0 <= 2^1 <= 2^2 <= 2^3 <= 2^4;

if(true) {
    putString("Success (0)\n");
} else {
    putString("Fail (0)\n");
}

if(b0 & b1) {
    putString("Success (1)\n");
} else {
    putString("Fail (1)\n");
}

if(b0 & b1) {
    putString("Success (2)\n");
    if(b0 & b1) {
        putString("Success (3)\n");
    } else {
        putString("Fail (3)\n");
    }
} else {
    putString("Fail (2)\n");
}
